{
    "name": "Front-End-Environment",
    "version": "1.0.0",
    "description": "creating project env ",
    "main": "app.js",
    "scripts": {
        "prestart": "babel-node build/start-message.js",
        "start": "npm-run-all --parallel security-check open:src lint:watch test:watch",
        "open:src": "babel-node build/dev-server.js",
        "lint": "esw webpack.config* src build --color",
        "lint:watch": "npm run lint -- --watch",
        "security-check": "nsp check",
        "test": "mocha --reporter progress build/test.js \"src/**/*.test.js\"",
        "test:watch": "npm run test -- --watch",
        "clean-dist": "rimraf ./dist && mkdir dist",
        "prebuild": "npm-run-all clean-dist test lint",
        "build": "babel-node build/build.js",
        "postbuild": "babel-node build/build-server.js",
        "deploy": "surge ./dist"
    },
    "repository": {
        "type": "git",
        "url": "https://github.com/mdabdulripon/project-with-Sass.git"
    },
    "author": "Abdul Ripon",
    "license": "ISC",
    "bugs": {
        "url": "https://github.com/mdabdulripon/project-with-Sass/issues"
    },
    "homepage": "https://github.com/mdabdulripon/project-with-Sass",
    "dependencies": {
        "animate.css": "^3.5.2",
        "bootstrap": "^4.0.0-alpha.6",
        "ionicons": "^3.0.0",
        "jquery": "^3.3.1",
        "jquery-validation": "^1.17.0",
        "popper.js": "^1.12.9",
        "tether": "^1.4.3",
        "whatwg-fetch": "^2.0.3",
        "wowjs": "^1.1.3"
    },
    "devDependencies": {
        "autoprefixer-loader": "^3.2.0",
        "babel-cli": "^6.26.0",
        "babel-core": "^6.26.0",
        "babel-loader": "^7.1.2",
        "babel-preset-env": "^1.6.1",
        "babel-preset-latest": "^6.24.1",
        "babel-register": "^6.26.0",
        "browser-sync": "^2.23.6",
        "browser-sync-webpack-plugin": "^2.0.1",
        "chai": "^4.1.2",
        "chalk": "^2.3.1",
        "compression": "^1.7.2",
        "css-loader": "^0.28.9",
        "eslint": "^4.18.0",
        "eslint-plugin-import": "^2.8.0",
        "eslint-watch": "^3.1.3",
        "expose-loader": "^0.7.4",
        "express": "^4.16.2",
        "extract-text-webpack-plugin": "^3.0.2",
        "file-loader": "^1.1.10",
        "html-webpack-plugin": "^2.30.1",
        "jsdom": "^11.6.2",
        "mocha": "^5.0.1",
        "node-sass": "^4.7.2",
        "nodemon": "^1.17.1",
        "npm-run-all": "^4.1.2",
        "open": "0.0.5",
        "postcss-loader": "^2.1.0",
        "sass-loader": "^6.0.6",
        "style-loader": "^0.20.2",
        "surge": "^0.20.1",
        "url-loader": "^0.6.2",
        "webpack": "^3.11.0",
        "webpack-dev-middleware": "^2.0.5",
        "webpack-hot-middleware": "^2.21.0",
        "webpack-md5-hash": "0.0.6"
    }
}
